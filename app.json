{
  "expo": {
    "name": "Arena Tracker",
    "slug": "arena-tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "scheme": "arena-tracker",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "googleServicesFile": "./firebase/google-services.json",
      "bundleIdentifier": "com.dingodeladouga.arenatracker",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/LT.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.dingodeladouga.arenatracker",
      "icon": "./src/assets/images/icon.png",
      "googleServicesFile": "./firebase/google-services.json",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": [
      
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/LT.png",
          "imageWidth": 200,
          "resizeMode": "contain"
        }
      ],
      "expo-font",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "7cb1dee6-ecab-46e4-8878-c9e7e4d2321b"
      }
    }
  }
}
